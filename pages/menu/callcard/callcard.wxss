/* pages/menu/callcard/callcard.wxss */
page {
  background-color: #f5f5f5;
}
.search {
  display: flex;
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  align-items: center;
  margin: 5px 0 10px;
}
.search-img {
  float: left;
  width: 20px;
  height: 20px;
  padding: 0 10px;
  line-height: 50px;
}
.search-input-box {
  flex: 3;
  width: 100%;
}
.detail {
  width: 690rpx;
  height: 150rpx;
  margin: 20rpx 30rpx;
  background-color: #fff;
  border-radius: 5px;
}
.detail-name-box {
  float: left;
  margin-left: 20rpx;
}
.detail-name {
  font-size: 15px;
}
.detail-btn-box {
  float: right;
  width: 95px;
  height: 35px;
  line-height: 12px;
  font-size: 15px;
  text-align: center;
  color: #fff;
  background-color: #ff5669;
  border-radius: 20px;
  margin: 37rpx 20rpx 0 0;
}
.accomplish-box {
text-align: center;
color: #94959a;
font-size: 16px;
padding-bottom: 16px;
}