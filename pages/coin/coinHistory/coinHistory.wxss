/* pages/coin/coinHistory/coinHistory.wxss */
page {
  background-color: #f5f5f5;
}
.coin-history-container{
  padding: 30rpx;
}

.item-history-profile{
  margin-bottom: 30rpx;
  background: #ffffff;
  border-radius: 20rpx;
  padding: 20rpx;
}

.left-profile{
  float: left;
  padding: 16rpx 0;
  width: 164rpx;
  height: 114rpx;
  line-height: 114rpx;
}
.left-profile-img{
  width: 100%;
  border-radius: 10rpx;
  border: 1px solid #eee;
  vertical-align: middle;
}

.right-profile{
  margin-left: 180rpx;
  line-height: 50rpx;
  font-size: 30rpx;
}

.right-right-box{
  color: #ff5569;
}